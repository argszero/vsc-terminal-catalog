{
  "name": "vsc-terminal-catalog",
  "displayName": "vsc-terminal-catalog",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld",
    "onCommand:itemClick"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration":{
      "title": "vsc-terminal-catalog",
      "properties": {
        "vsc-terminal-catalog.catalog-file":{
          "type": "string",
          "default": "terminals.json",
          "description": "终端列表配置文件"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vsc-terminal-catalog-bar",
          "title": "terminal catalog bar",
          "icon": "resources/console.svg"
        }
      ]
    },
    "views": {
      "vsc-terminal-catalog-bar": [
        {
          "id": "vsc-terminal-catalog-treeView",
          "name": "terminal treeView",
          "when": ""
        }
      ]
    },
    "commands": [ ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}